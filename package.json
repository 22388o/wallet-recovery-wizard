{
  "name": "wallet-recovery-wizard",
  "description": "An UI-based desktop app for BitGo Recoveries",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dev": "nf start",
