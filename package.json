    "buffer": "^5.0.8",
    "dns": "^0.2.2",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-mailto": "^0.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0",
    "react-select": "^1.2.1",
    "react-transition-group": "^2.2.1",
    "reactstrap": "^5.0.0-beta",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "superagent": "^3.8.2",
    "superagent-as-promised": "^4.0.0"
